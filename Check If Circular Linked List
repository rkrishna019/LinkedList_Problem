class GfG
{
    boolean isCircular(Node head)
    {
	// Your code here
	if(head == null) return false;
//	if(head.next == head) return true;
	Node curr = head;
	while(curr != null){
	    
	    curr = curr.next;
	    if(curr == head){
	        return true;
	         
	    }
	}
	return false;
    }
}
